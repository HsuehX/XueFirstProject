<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.example.lenovo.textviewspannerdalogexercise.view.CustomScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F5F5F5"
            android:orientation="vertical">.

            <!--<include layout="@layout/bar_normal" />-->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/margin">

                <ImageView
                    android:id="@+id/iv_rider_portrait"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/rider_portrait" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/iv_rider_portrait"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_rider_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="齐连明"
                        android:textColor="@color/title_text"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:id="@+id/tv_label_tpye"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/label_background"
                        android:gravity="center"
                        android:padding="2sp"
                        android:text="蜂鸟配送"
                        android:textColor="@color/white"
                        android:textSize="@dimen/font_size_12" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:text="已对骑手匿名"
                    android:textColor="@color/black_gray"
                    android:textSize="@dimen/font_size_14" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin"
                android:layout_marginRight="@dimen/margin"
                android:background="@color/white"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="30dp"
                android:paddingTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="配送评价"
                    android:textColor="@color/title_text"
                    android:textSize="@dimen/font_size_20"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="50dp"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="25dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ll_evaluation_very_bad"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_evaluation_very_bad"
                            android:layout_width="55dp"
                            android:layout_height="55dp"
                            android:background="@drawable/very_bad" />

                        <TextView
                            android:id="@+id/tv_evaluation_very_bad"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="非常差"
                            android:textColor="@color/black_gray" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_evaluation_general"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_evaluation_general"
                            android:layout_width="55dp"
                            android:layout_height="55dp"
                            android:background="@drawable/general" />

                        <TextView
                            android:id="@+id/tv_evaluation_general"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="11dp"
                            android:text="一般"
                            android:textColor="@color/black_gray" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_evaluation_awesome"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_evaluation_awesome"
                            android:layout_width="55dp"
                            android:layout_height="55dp"
                            android:background="@drawable/awesome" />

                        <TextView
                            android:id="@+id/tv_evaluation_awesome"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="超赞"
                            android:textColor="@color/evaluation_select" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="30dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_fast_delivery"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/evaluation_label_type_distance"
                            android:layout_marginRight="@dimen/evaluation_label_type_distance"
                            android:layout_weight="1"
                            android:background="@drawable/evaluation_label_type"
                            android:gravity="center"
                            android:padding="@dimen/evaluation_item_indistance"
                            android:text="送餐快"
                            android:textColor="@color/black_gray" />

                        <TextView
                            android:id="@+id/tv_meals_complete"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/evaluation_label_type_distance"
                            android:layout_marginRight="@dimen/evaluation_label_type_distance"
                            android:layout_weight="1"
                            android:background="@drawable/evaluation_label_type"
                            android:gravity="center"
                            android:padding="@dimen/evaluation_item_indistance"
                            android:text="餐品完好"
                            android:textColor="@color/black_gray" />


                        <TextView
                            android:id="@+id/tv_good_service_attitude"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/evaluation_label_type_distance"
                            android:layout_marginRight="@dimen/evaluation_label_type_distance"
                            android:layout_weight="1"
                            android:background="@drawable/evaluation_label_type"
                            android:gravity="center"
                            android:padding="@dimen/evaluation_item_indistance"
                            android:text="服务态度好"
                            android:textColor="@color/black_gray" />


                        <TextView
                            android:id="@+id/tv_arrive_on_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/evaluation_label_type_distance"
                            android:layout_marginRight="@dimen/evaluation_label_type_distance"
                            android:layout_weight="1"
                            android:background="@drawable/evaluation_label_type"
                            android:gravity="center"
                            android:padding="@dimen/evaluation_item_indistance"
                            android:text="准时到达"
                            android:textColor="@color/black_gray" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_wearing_professional"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/evaluation_label_type_distance"
                            android:layout_marginRight="@dimen/evaluation_label_type_distance"
                            android:background="@drawable/evaluation_label_type"
                            android:gravity="center"
                            android:padding="@dimen/evaluation_item_indistance"
                            android:text="穿着专业"
                            android:textColor="@color/black_gray" />

                        <TextView
                            android:id="@+id/tv_neatly_dressed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/evaluation_label_type_distance"
                            android:layout_marginRight="@dimen/evaluation_label_type_distance"
                            android:background="@drawable/evaluation_label_type"
                            android:gravity="center"
                            android:padding="@dimen/evaluation_item_indistance"
                            android:text="衣着整洁"
                            android:textColor="@color/black_gray" />


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/evaluation_label_type_distance"
                            android:layout_marginRight="@dimen/evaluation_label_type_distance"
                            android:background="@drawable/evaluation_label_type"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:padding="@dimen/evaluation_item_indistance">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/text_evaluation"
                                android:layout_marginRight="2dp"/>

                            <TextView
                                android:id="@+id/tv_text_evaluation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="文字评价"
                                android:textColor="@color/evaluation_label_type_selected" />
                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="@dimen/margin"
                android:layout_marginRight="@dimen/margin"
                android:layout_marginTop="20dp">

                <ImageView
                    android:id="@+id/iv_shop_head"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_centerVertical="true"
                    android:background="@drawable/shop_head_icon" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/iv_shop_head"
                    android:text="私房小厨（浑南店）"
                    android:textColor="@color/light_black"
                    android:textSize="@dimen/font_size_16" />


                <CheckBox
                    android:id="@+id/cb_consent_agreement"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="  匿名评价"
                    android:textColor="@color/black_gray"
                    android:textSize="@dimen/font_size_14" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin"
                android:layout_marginRight="@dimen/margin"
                android:background="@color/white"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="20dp"
                android:paddingTop="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="商家评价"
                    android:textColor="@color/title_text"
                    android:textSize="@dimen/font_size_20"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:orientation="vertical">


                    <com.iarcuschin.simpleratingbar.SimpleRatingBar
                        android:id="@+id/srb_merchant_evaluation"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        app:srb_borderColor="@color/evaluation_select"
                        app:srb_fillColor="@color/evaluation_select"
                        app:srb_rating="5"
                        app:srb_stepSize="1"
                        app:starImageSize="35dp"
                        app:starPadding="10dp" />

                    <TextView
                        android:id="@+id/tv_merchant_evaluation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="超赞"
                        android:textColor="@color/evaluation_select" />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="包装"
                        android:textColor="@color/light_black"
                        android:textSize="@dimen/font_size_16" />

                    <com.iarcuschin.simpleratingbar.SimpleRatingBar
                        android:id="@+id/srb_package_evaluation"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        app:srb_borderColor="@color/evaluation_select"
                        app:srb_fillColor="@color/evaluation_select"
                        app:srb_rating="5"
                        app:srb_stepSize="1"
                        app:starImageSize="35dp"
                        app:starPadding="10dp" />

                    <TextView
                        android:id="@+id/tv_package_evaluation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="超赞"
                        android:textColor="@color/evaluation_select" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="味道"
                        android:textColor="@color/light_black"
                        android:textSize="@dimen/font_size_16" />

                    <com.iarcuschin.simpleratingbar.SimpleRatingBar
                        android:id="@+id/srb_taste_evaluation"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        app:srb_borderColor="@color/evaluation_select"
                        app:srb_fillColor="@color/evaluation_select"
                        app:srb_rating="5"
                        app:srb_stepSize="1"
                        app:starImageSize="35dp"
                        app:starPadding="10dp" />

                    <TextView
                        android:id="@+id/tv_taste_evaluation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="超赞"
                        android:textColor="@color/evaluation_select" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin"
                            android:layout_marginRight="@dimen/margin"
                            android:background="@drawable/text_evaluation_bg"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/cabinet_describe_et"
                                android:layout_width="match_parent"
                                android:layout_height="180dp"
                                android:layout_alignLeft="@+id/relativeLayout"
                                android:layout_alignParentTop="true"
                                android:layout_alignStart="@+id/relativeLayout"
                                android:layout_margin="10dp"
                                android:background="@null"
                                android:gravity="top"
                                android:hint="请输入描述内容"
                                android:maxLength="200"
                                android:text=""
                                android:textSize="@dimen/font_size_16" />
                        </LinearLayout>

                        <RelativeLayout
                            android:id="@+id/relativeLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_marginBottom="20dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp">

                            <ImageView
                                android:id="@+id/iv_add_photo"
                                android:layout_width="90dp"
                                android:layout_height="90dp"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/add_img" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:layout_toLeftOf="@+id/tv_get_gold"
                                android:paddingBottom="10dp"
                                android:text="评价文字/图片共得"
                                android:textColor="@color/light_black" />

                            <TextView
                                android:id="@+id/tv_get_gold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentRight="true"
                                android:paddingBottom="10dp"
                                android:text=" 5金币"
                                android:textColor="@color/text_evaluation_get_gold" />
                        </RelativeLayout>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin"
                android:layout_marginRight="@dimen/margin"
                android:layout_marginTop="20dp"
                android:background="@color/white"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="20dp"
                android:paddingTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="有没有不错或比较差的食物？"
                    android:textColor="@color/title_text"
                    android:textSize="@dimen/font_size_20"
                    android:textStyle="bold" />

                <com.example.lenovo.textviewspannerdalogexercise.view.CustomListView
                    android:id="@+id/lv_satisfaction_level"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin"
                    android:layout_marginRight="@dimen/margin"
                    android:layout_marginTop="15dp"
                    android:background="@color/white"
                    android:divider="@color/transparent"></com.example.lenovo.textviewspannerdalogexercise.view.CustomListView>
            </LinearLayout>

